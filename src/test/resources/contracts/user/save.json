{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "id": "PPd1o0jad232h2z5qh3x30q663",
      "name": "Elwood Stamm",
      "email": "herman.mraz@hotmail.com"
    }
  ],
  "required": [
    "id",
    "name",
    "email"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The id schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "PPd1o0jad232h2z5qh3x30q663"
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "Elwood Stamm"
      ]
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The email schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "herman.mraz@hotmail.com"
      ]
    }
  },
  "additionalProperties": true
}